{
  "name": "@kubosho/configs",
  "description": "My Web frontend tool configs",
  "version": "3.2.1",
  "license": "MIT",
  "author": "kubosho",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test:eslint": "ESLINT_USE_FLAT_CONFIG=true eslint --config ./eslint/examples/eslint.config.mjs ./eslint/examples/src/*",
    "print:eslint": "ESLINT_USE_FLAT_CONFIG=true eslint --config ./eslint/examples/eslint.config.mjs --print-config ./eslint/examples/eslint.config.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./eslint/configs/a11y.js",
    "./eslint/configs/import.js",
    "./eslint/configs/react.mjs",
    "./eslint/configs/typescript.js",
    "./eslint/index.mjs",
    "./typescript/tsconfig.json",
    "./prettier/index.js"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.5.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.2",
    "globals": "^15.8.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "semantic-release": "^24.0.0"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "scope": "no-release",
              "release": false
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "scope": "deps",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "‚ú® Features",
                "hidden": false
              },
              {
                "type": "fix",
                "section": "üêõ Bug Fixes",
                "hidden": false
              },
              {
                "type": "chore",
                "scope": "deps",
                "section": "üìö Some changes",
                "hidden": false
              },
              {
                "type": "refactor",
                "section": "üìö Some changes",
                "hidden": false
              },
              {
                "type": "docs",
                "section": "üìö Some changes",
                "hidden": false
              },
              {
                "type": "style",
                "hidden": true
              },
              {
                "type": "perf",
                "hidden": true
              },
              {
                "type": "test",
                "hidden": true
              }
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
