{
  "name": "@kubosho/configs",
  "description": "My Web frontend tool configs",
  "version": "3.13.0",
  "license": "MIT",
  "author": "kubosho",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test:ts": "tsc --noEmit",
    "test:eslint": "vitest run",
    "lint:eslint": "ESLINT_USE_FLAT_CONFIG=true eslint --config ./eslint/examples/eslint.config.mjs ./eslint/examples/src/*",
    "print:eslint": "ESLINT_USE_FLAT_CONFIG=true eslint --config ./eslint/examples/eslint.config.mjs --print-config ./eslint/examples/eslint.config.mjs",
    "format": "prettier --write ."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./eslint/configs/*",
    "./eslint/index.mjs",
    "./markuplint/index.mjs",
    "./prettier/index.js",
    "./semantic_release/index.mjs",
    "./textlint/index.js",
    "./typescript/tsconfig.json"
  ],
  "dependencies": {
    "@eslint/compat": "^1.1.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "markuplint": "^4.9.3",
    "prettier": "^3.3.0",
    "react": "^18.3.0",
    "semantic-release": "^24.1.0",
    "textlint": "^14.2.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-no-dead-link": "^5.2.0",
    "textlint-rule-no-doubled-joshi": "^5.1.0",
    "textlint-rule-preset-jtf-style": "^2.3.14",
    "typescript": "^5.5.0",
    "typescript-eslint": "^7.17.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.5.4",
    "vitest": "^2.0.5"
  }
}
